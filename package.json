    "engines": {
        "node": ">=18.0.0",
        "npm": ">=8.0.0"
    },
    "scripts": {
        "start": "http-server -p 8080 -c-1 --index index.html",
        "start-py": "python3 server/server.py",
        "start-js": "node server/node.js", 
        "start-gestos": "python3 /Users/mac/Documents/GitHub/MAIRA_git/Server/detectorGestos.py",
        "dev": "concurrently \"npm run start-py\" \"npm run start-js\" \"npm start\"",
        "dev-https": "concurrently \"npm run start-py-https\" \"npm run start-js\"",
        "start-py-https": "python3 server/serverhttps.py",
        "install-gesture-deps": "python3 server/install_deps.py",
        "build": "npm run optimize:assets && npm run prepare:client",
        "build:production": "npm run optimize:assets && npm run prepare:client && npm run minify",
        "optimize:assets": "echo 'Optimizing assets for production...'",
        "prepare:client": "echo 'Preparing client files for static hosting...'",
        "minify": "echo 'Minifying JavaScript and CSS files...'",
        "test": "npm run test:client && npm run test:integration",
        "test:client": "echo 'Running client-side tests...'",
        "test:integration": "python dev-tools/test_runner_local.py",
        "test:urls": "python scripts/test_urls.py",
        "lint": "eslint Client/js/**/*.js",
        "lint:fix": "eslint Client/js/**/*.js --fix",
        "clean": "rm -rf node_modules/.cache && rm -rf dist/",
        "deploy": "git push origin main",
        "health-check": "curl -f https://maira-3e76.onrender.com/health || exit 1"
    },
